<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @user.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @user.last_name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Description:</strong>
  <%= @user.description %>
</p>

<% if params[:id] == current_user.id.to_s %>
	<%= link_to 'Edit', edit_user_registration_path %> |
<% end %>
<%= link_to 'Back', :back %>

<% if @liseve.length != 0 %>
	<br/>
	<br/>
	<table>
		<tr><td><b>Date &nbsp;</b></td><td><b>Title</b></td></tr>
		<% @liseve.each do |uneve| %>
			<tr><td nowrap><b><%= link_to uneve.start_date.to_s[0,16], event_path(uneve) %> &nbsp;</b></td><td><%= uneve.title %></td></tr>
		<% end %>
	</table>
<% end %>
